{
  "name": "mcp-server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mcp-server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      }
    },
    "serve": {
      "executor": "nx:run-script",
      "options": {
        "script": "serve"
      }
    },
    "serve-mcp": {
      "executor": "nx:run-script",
      "options": {
        "script": "serve:mcp"
      }
    },
    "test": {
      "executor": "nx:run-script",
      "options": {
        "script": "test"
      }
    },
    "lint": {
      "executor": "nx:run-script",
      "options": {
        "script": "lint"
      }
    }
  },
  "tags": ["app", "server"]
}
