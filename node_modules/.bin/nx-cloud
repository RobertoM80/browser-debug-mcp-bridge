#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/bronz/Documents/progetti/browser-debug-mcp-bridge/node_modules/.pnpm/nx@20.8.4/node_modules/nx/bin/node_modules:/proc/cygdrive/c/Users/bronz/Documents/progetti/browser-debug-mcp-bridge/node_modules/.pnpm/nx@20.8.4/node_modules/nx/node_modules:/proc/cygdrive/c/Users/bronz/Documents/progetti/browser-debug-mcp-bridge/node_modules/.pnpm/nx@20.8.4/node_modules:/proc/cygdrive/c/Users/bronz/Documents/progetti/browser-debug-mcp-bridge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/bronz/Documents/progetti/browser-debug-mcp-bridge/node_modules/.pnpm/nx@20.8.4/node_modules/nx/bin/node_modules:/proc/cygdrive/c/Users/bronz/Documents/progetti/browser-debug-mcp-bridge/node_modules/.pnpm/nx@20.8.4/node_modules/nx/node_modules:/proc/cygdrive/c/Users/bronz/Documents/progetti/browser-debug-mcp-bridge/node_modules/.pnpm/nx@20.8.4/node_modules:/proc/cygdrive/c/Users/bronz/Documents/progetti/browser-debug-mcp-bridge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nx/bin/nx-cloud.js" "$@"
else
  exec node  "$basedir/../nx/bin/nx-cloud.js" "$@"
fi
